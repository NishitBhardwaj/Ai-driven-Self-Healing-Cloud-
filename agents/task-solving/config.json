{
  "agent": {
    "id": "task-solving-agent",
    "name": "Task-Solving Agent",
    "description": "Parses user tasks and converts them into agent actions"
  },
  "llm": {
    "provider": "openrouter",
    "model": "openai/gpt-3.5-turbo",
    "max_tokens": 1000,
    "temperature": 0.7
  },
  "task_processing": {
    "max_concurrent_tasks": 10,
    "timeout_seconds": 300,
    "retry_attempts": 3
  },
  "message_bus": {
    "subscribe_topics": [
      "task.created",
      "task.completed"
    ],
    "publish_topics": [
      "task.created",
      "task.completed"
    ]
  }
}

